#!/bin/sh

# List of aliases

alias v="nvim" \
	g="git" \
	in="task add +inbox" \
	tt="task -inbox -someday" \
	ti="task inbox" \
	install="sudo apt-get install" \
	p="sudo pacman" \
	cre="crontab -e" \
	mg="nvim -c MagitOnly" \
	tw="nvim -c TW" \
	wd="nvim -c VimwikiMakeDiaryNote" \
	wal="wal --vte" \
	z='zathura' \
	SS='sudo systemctl' \
	ls='ls --color=auto' \
	duck='sr duckduckgo' \
	pirate='pirate-get -t' \
	cal='calcurse -D ~/.config/calcurse' \
	gp='git push origin master' \
	f='vifmrun' \
	tra="transmission-remote-cli" \
	pp="pip3 install --user"





