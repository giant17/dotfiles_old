
# APPS

# terminal 
super + Return
	$TERMINAL

# launcher
super + d
	dmenu_run

# tmux sessions
super + super
	$TERMINAL -e findSessions

# music 
super + shift + m
	$TERMINAL -e launchTmux 'music' 'cmus'

# browser cli 
super + w
	$TERMINAL -e launchTmux 'web' 'w3m -v'

# Browser gui
super + shift + w
	$BROWSER

# rss  
super + n
	$TERMINAL -e launchTmux 'news' 'newsboat'

# file manager
super + r
	$TERMINAL -e vifm




## MANAGEMENT

# Inbox
super + y; y
	addinbox

# Process
super + y; p
	processinbox


super  + F1
	new_app.sh

# File manager
super + r
	$TERMINAL -e vifmrun

super + e
	$TERMINAL -e neomutt
super + m
	$TERMINAL -e ncmpcpp
super + n
	$TERMINAL -e newsboat

# GTD shortcuts
super + y; p
	process.sh
super + y; w
	workout.sh
super + y; f
	getFlashcard.sh
