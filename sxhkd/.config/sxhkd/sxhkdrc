#weeekly review - add to calendar


# Wiki shortcuts
super + v
	$TERMINAL -e nvim -c VimwikiIndex

# Reload config
super + F6
	torwrap.sh
super + shift + Escape
	quitMenu.sh
# super + grave; e
# 	rofimoji
super + grave
	getFontawesome.sh
super + F9
	mount.sh
super + F10
	unmount.sh
super + Print
	record.sh
# Thinkpad german Druck
shift + Print
	maimpick.sh
super + F2
	monitor.sh --auto
super + F3
	monitor.sh --manual
super + F12
	$TERMINAL -e nmtui

# Volume

XF86AudioMute
	lmc.sh mute
XF86AudioLowerVolume
	lmc.sh down
XF86AudioRaiseVolume
	lmc.sh up

XF86MonBrightnessDown
	xbacklight -dec 15
XF86MonBrightnessUp
	xbacklight -inc 15
