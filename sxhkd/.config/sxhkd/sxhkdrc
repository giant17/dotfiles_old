	$TERMINAL -e nmtui

# Volume

XF86AudioMute
	lmc.sh mute
XF86AudioLowerVolume
	lmc.sh down
XF86AudioRaiseVolume
	lmc.sh up

XF86MonBrightnessDown
	xbacklight -dec 15
XF86MonBrightnessUp
	xbacklight -inc 15
