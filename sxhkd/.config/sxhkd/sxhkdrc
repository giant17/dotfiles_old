super + Return
	$TERMINAL
super + d
	dmenu_run
# music
super + m
	$TERMINAL -e launchTmux 'music' 'cmus'

# browser cli
super + w
	$TERMINAL -e launchTmux 'web' 'w3m -config $HOME/.config/w3m/config -v'

# Browser gui
super + shift + w
	$BROWSER

# calendar
super + y
	$TERMINAL -e launchTmux 'calendar' 'calcurse -D $HOME/.config/calcurse'

# rss
super + n
	$TERMINAL -e newsboat

# file manager
super + r
	$TERMINAL -e $FILE

# mail
super + e
	$TERMINAL -e neomutt

# Matlab
super + shift + m
	$TERMINAL -e launchTmux 'matlab' "vifm $REPOS/matlab"

# Brainstorm
super + b
	tmuxBrainstorm

