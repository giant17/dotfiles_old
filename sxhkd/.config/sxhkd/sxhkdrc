super + Return
	$TERMINAL
super + d
	dmenu_run
# music
super + m
	$TERMINAL -e startTmux 'music' 'cmus'

# browser cli
super + w
	$TERMINAL -e startTmux 'web' 'w3m -config $HOME/.config/w3m/config -v'

# Browser gui
super + shift + w
	$BROWSER

# Inbox
super + y
	addinbox


# calendar
super + shift  + y
	$TERMINAL -e startTmux 'calendar' 'calcurse -D $HOME/.config/calcurse -c $HOME/.local/share/calcurse/apts'

# rss
super + n
	$TERMINAL -e newsboat -u $HOME/.local/share/newsboat/urls

# file manager
super + r
	$TERMINAL -e $FILE

# mail
super + e
	$TERMINAL -e neomutt

# Matlab
super + shift + m
	$TERMINAL -e startTmux 'matlab' "lf $REPOS/matlab"

# Brainstorm
super + b
	tmuxBrainstorm

# Slock
super + shift + x
	slock

super  + F1
	checkstatusgit --dmenu

# GTD shortcuts
super + y; p
	process.sh
super + y; w
	workout.sh
super + y; f
	getFlashcard.sh
#super + y; j
# 5 minute journal
#super + y; r
#weeekly review - add to calendar


# Wiki shortcuts
super + v
	$TERMINAL -e nvim -c VimwikiIndex

# Reload config
super + F6
	torwrap.sh
super + shift + Escape
	dquit
# super + grave; e
# 	rofimoji
super + grave
	getFontawesome.sh
super + F9
	mount
super + F10
	unmount
super + Print
	record.sh
# Thinkpad german Druck
shift + Print
	maimpick.sh
super + F2
	monitor.sh --auto
super + F3
	displayselect
super + F12
	$TERMINAL -e nmtui

# Volume

XF86AudioMute
	lmc mute
XF86AudioLowerVolume
	lmc down 5
XF86AudioRaiseVolume
	lmc up 5

XF86MonBrightnessDown
	xbacklight -dec 15
XF86MonBrightnessUp
	xbacklight -inc 15
